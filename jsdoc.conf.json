{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "./src/"
        ],
        "includePattern": "\\.js$",
        "excludePattern": "(assets)"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "template": "./node_modules/docdash",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    },
    "docdash": {
        "collapse": true,
        "search": true,
        "sectionOrder": [
            "Namespaces",
            "Modules",
            "Classes"
        ]
    }
}